{
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, ColumnLimit: 0 }",
    "cmake.debugConfig": {
        "args": [
            "./tools/config.yaml"
        ],
        "cwd": "${workspaceFolder}"
    },
    "files.associations": {
        "*.tcc": "cpp",
        "bitset": "cpp",
        "chrono": "cpp",
        "algorithm": "cpp",
        "__bit_reference": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp"
    }
}