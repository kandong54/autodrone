{
  "name": "web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "proto-linux": "protoc --proto_path=./protos $(find ./protos -iname \"*.proto\") --js_out=import_style=commonjs:./src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/protos",
    "proto-macos": "protoc --proto_path=./protos $(find ./protos -iname \"*.proto\") --js_out=import_style=commonjs:./src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/protos",
    "proto-windows": "protoc --proto_path=./protos $(Get-ChildItem -Path ./protos -Include *.proto -Name) --js_out=import_style=commonjs:./src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/protos"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/cdk": "^15.0.2",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/forms": "^15.0.3",
    "@angular/material": "^15.0.2",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/router": "^15.0.4",
    "@types/colormap": "^2.3.1",
    "@types/google-protobuf": "^3.15.6",
    "colormap": "^2.3.2",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.4.2",
    "rxjs": "~7.6.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular/cli": "^15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@types/jasmine": "^4.3.1",
    "@types/node": "^18.11.15",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4"
  }
}
