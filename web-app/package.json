{
  "name": "web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "proto-linux": "protoc --proto_path=./protos $(find ./protos -iname \"*.proto\") --js_out=import_style=commonjs:./src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/protos",
    "proto-windows": "protoc --proto_path=./protos $(Get-ChildItem -Path ./protos -Include *.proto -Name) --js_out=import_style=commonjs:./src/protos --grpc-web_out=import_style=typescript,mode=grpcwebtext:./src/protos"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.2",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "^14.2.2",
    "@angular/compiler": "^14.2.2",
    "@angular/core": "^14.2.2",
    "@angular/forms": "^14.2.2",
    "@angular/material": "^14.2.2",
    "@angular/platform-browser": "^14.2.2",
    "@angular/platform-browser-dynamic": "^14.2.2",
    "@angular/router": "^14.2.2",
    "@types/google-protobuf": "^3.15.6",
    "google-protobuf": "^3.21.0",
    "grpc-web": "^1.3.1",
    "rxjs": "~7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.2",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.7.18",
    "jasmine-core": "~4.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.3"
  }
}
